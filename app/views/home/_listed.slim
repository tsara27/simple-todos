- unless listed.count.zero?
  - listed.each do |item| 
    li.collection-item 
      = item.text
      = link_to '<i class="mdi-navigation-arrow-forward pink-text text-darken-4" id="move_to_doing"></i>'.html_safe, api_todo_path(id: item, status: "doing"), class: "secondary-content tooltipped move_to", data: { remote: true, tooltip: "Move to doing", delay: 10 }, method: :patch